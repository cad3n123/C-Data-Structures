{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build-main",
            "command": "make",
            "args": [
                "main"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Manually set Make build for C project"
        }
    ]
}
